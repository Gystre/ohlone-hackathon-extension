{
    "name": "extension thing",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["tabs"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["init.js"]
        }
    ],
    "action": {
        "default_title": "extension thing",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
