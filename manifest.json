{
    "name": "extension thing",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["tabs"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jquery-3.6.0.min.js", "init.js"]
        }
    ],
    "background": {
        "scripts": ["jquery-3.6.0.min.js", "background.js"]
    },
    "browser_action": {
        "default_title": "extension thing",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "web_accessible_resources": ["data.json", "popup.html"]
}
